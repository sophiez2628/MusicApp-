<h1>Sign In</h1>

<form action="/session" method="POST">
  <%= auth_token %>
  <label for="user_email">Email</label>
  <input type="text"
         name="user[email]"
         value="<%= @user.email %>"
         id="user_email"><br>
  <label for="user_password">Password</label>
  <input type="password"
         name="user[password]"
         value="<%= @user.password %>"
         id="user_password"><br>
  <input type="submit" value="Log In">
</form>
